    "generate-docs": "jsdoc node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blacky2810/manage-giveaways.git"
  },
