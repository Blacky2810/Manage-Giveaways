    "deepmerge": "^4.2.2",
    "serialize-javascript": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.6.0",
    "discord.js": "^13.5.0",
    "eslint": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "jsdoc": "^3.6.4",
    "jsdoc-skyceil": "github:Androz2091/jsdoc-skyceil",
    "node": "^16.13.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "discord.js": ">=13.5.0"
